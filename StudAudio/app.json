{
  "expo": {
    "name": "StudAudio",
    "slug": "StudAudio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/iosIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/finalSplashh.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.superyasser.StudAudio",
      "icon": "./assets/icons/iosIcon.png",
      "splash": {
        "image": "./assets/icons/finalSplashh.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "runtimeVersion": "1.0.0"
    },
    "plugins": [
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "speechRecogntionPermission": "Allow $(PRODUCT_NAME) to securely recognize user speech"
        }
      ]
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/finalSplashh.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.superyasser.StudAudio",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "runtimeVersion": {
        "policy": "1.0.0"
      }
    },
    "web": {
      "favicon": "./assets/icons/iosIcon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2fffda26-f394-46d2-9f3d-aa66ab712409"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/2fffda26-f394-46d2-9f3d-aa66ab712409"
    }
  }
}
